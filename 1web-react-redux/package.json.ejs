{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "private": true,
  "browserslist": [
    "chrome >= 70"
  ],
  "workspaces": [
    "./mock",
    "./public"
  ],
  "scripts": {
    "lint:type": "tsc --project ./src --noEmit --emitDeclarationOnly false",
    "lint:es": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts,tsx}",
    "lint:style": "cross-env NODE_ENV=production stylelint --fix --cache **/*.{css,<%= css %>}",
    "lint:json": "prettier --write **/*.json",
    "mock": "elux mock --watch",
    "dev": "../node_modules/.bin/elux dev",
    "start": "run-p mock dev",
    "build": "elux build",
    "demo-local": "node ./dist/local/start.js",
    "demo-mock-local": "run-p mock demo-local"
  },
  "dependencies": {
    "@elux/react-redux-web": "^0.1.124",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "@types/express": "~4.17.9",
    "@elux/babel-preset": "~0.1.5",
    "@elux/cli-utils": "~0.0.7",
    "@elux/cli-mock": "~0.0.4",
    "@elux/cli-webpack": "~0.0.3",
    "@elux/eslint-plugin": "~0.1.20",
    "@elux/stylelint-config": "~0.1.7",
    "prettier": "^2.4.0",
    "autoprefixer": "10.4.0",
    "<%= css %>": "~3.12.2",
    "<%= css %>-loader": "~7.1.0",
    "postcss-<%= css %>": "~5.0.0",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.0",
    "typescript": "~4.2.0"
  }
}
