{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "private": true,
  "browserslist": [
    "chrome >= 70"
  ],
  "workspaces": [
    "./mock",
    "./public"
  ],
  "scripts": {
    "lint:type": "tsc --project ./src --noEmit --emitDeclarationOnly false",
    "lint:es": "cross-env NODE_ENV=production eslint --fix --cache **/*.{js,ts,tsx<%= render==='tpl'?',vue':''%>}",
    "lint:css": "cross-env NODE_ENV=production stylelint --fix --cache **/*.{css,<%= css %><%= render==='tpl'?',vue':''%>}",
    "lint:json": "prettier --write **/*.json",
    "mock": "elux mock --watch",
    "dev": "elux dev",
    "start": "run-p mock dev",
    "build": "elux build",
    "demo-local": "node ./dist/local/start.js",
    "demo-mock-local": "run-p mock demo-local"
  },
  "dependencies": {
  <%_ if(framework==='reactRedux'){ -%>
    "@elux/react-redux-web": "^0.1.127",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
  <%_ }else if(framework==='vueVuex'){ -%>
    "@elux/vue-vuex-web": "^0.1.127",
    "vue": "^3.2.22",
    <%_ } -%>
    "axios": "^0.21.1",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
  <%_ if(framework==='reactRedux'){ -%>
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
  <%_ } -%>
    "@types/express": "~4.17.9",
    "@elux/cli": "~0.1.47",
    "@elux/cli-utils": "~0.1.47",
    "@elux/cli-webpack": "~0.1.48",
    "@elux/cli-mock": "~0.1.26",
    "@elux/babel-preset": "~0.1.7",
    "@elux/eslint-plugin": "~0.1.23",
    "@elux/stylelint-config": "~0.1.10",
    "prettier": "^2.4.0",
    "autoprefixer": "10.4.0",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.0",
    "typescript": "~4.2.0",
  <%_ if(css==='less'){ -%>
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "postcss-less": "~5.0.0"
  <%_ }else{ -%>
    "sass": "~1.43.0",
    "sass-loader": "~12.3.0",
    "postcss-scss": "~4.0.0"
  <%_ } -%>
  }
}
