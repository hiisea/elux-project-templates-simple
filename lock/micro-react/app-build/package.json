{
  "name": "@micro-react/app-build",
  "version": "1.0.0",
  "description": "",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "__workspaces": [
    "./public"
  ],
  "browserslist": [
    "chrome >= 70"
  ],
  "scripts": {
    "lint:type": "tsc --project ./src --noEmit --emitDeclarationOnly false",
    "lint:es": "cross-env NODE_ENV=production eslint --fix --cache \"**/*.{js,ts,tsx}\"",
    "lint:css": "cross-env NODE_ENV=production stylelint --fix --cache \"**/*.{css,less}\"",
    "lint:json": "prettier --write **/*.json",
    "demote": "elux demote",
    "dev": "elux dev",
    "build": "elux build",
    "build:analyzer": "elux build -a",
    "mock": "elux mock --watch",
    "start": "run-p mock dev",
    "dist": "node ./dist/local/start.js",
    "demo": "run-p mock dist"
  },
  "dependencies": {
    "@elux/react-web": "^2.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "query-string": "^7.1.1",
    "path-to-regexp": "^6.2.0",
    "@micro-react/stage": "^1.0.0",
    "@micro-react/article": "^1.0.0",
    "@micro-react/my": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@elux/cli": "^2.0.0",
    "@elux/cli-utils": "^2.0.0",
    "@elux/cli-webpack": "^2.0.0",
    "@elux/cli-mock": "^2.0.0",
    "@elux/babel-preset": "^1.0.2",
    "@elux/eslint-plugin": "^1.0.1",
    "@elux/stylelint-config": "^1.1.1",
    "npm-run-all": "~4.1.5",
    "cross-env": "~7.0.0",
    "typescript": "~4.2.0",
    "prettier": "~2.4.0",
    "autoprefixer": "~10.4.0",
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "sass": "~1.43.0",
    "sass-loader": "~12.3.0"
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}