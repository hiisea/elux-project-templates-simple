{
  "name": "micro-vue",
  "version": "1.0.0",
  "description": "",
  "author": "hiisea <hiisea@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "browserslist": [
    "chrome >= 70"
  ],
  "workspaces": [
    "app-api",
    "app-build",
    "app-runtime",
    "basic-team",
    "article-team",
    "user-team",
    "app-build/public",
    "app-runtime/public",
    "basic-team/public",
    "article-team/public",
    "user-team/public"
  ],
  "scripts": {
    "mock": "lerna run mock --scope @micro-vue/app-runtime --parallel",
    "dev": "lerna run dev --scope @micro-vue/app-runtime --scope @micro-vue/article-team --scope @micro-vue/user-team --parallel",
    "build": "lerna run build --scope @micro-vue/app-runtime --scope @micro-vue/article-team --scope @micro-vue/user-team --parallel",
    "dist": "lerna run dist --scope @micro-vue/app-runtime --scope @micro-vue/article-team --scope @micro-vue/user-team --parallel",
    "start": "run-p mock dev",
    "demo": "run-p mock dist"
  },
  "devDependencies": {
    "npm-run-all": "~4.1.5",
    "lerna": "~3.22.1"
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}
